{
    "extends": [
        "google",
        "eslint:recommended",
        "plugin:prettier/recommended",
        "plugin:react/recommended",
        "plugin:react-hooks/recommend"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },

    "rules": {
        "require-jsdoc": 0,
        "prettier/prettier": [
            "error",
            {
                "singleQuote": true,
                "parser": "flow",
                "endOfLine": "auto"
            }
        ]
    }
}
