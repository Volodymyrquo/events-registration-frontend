{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["google", "eslint:recommended", "plugin:prettier/recommended"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "parser": "babel-eslint",
    "rules": {
        "require-jsdoc": 0,
        "prettier/prettier": [
            "error",
            {
                "singleQuote": true,
                "parser": "flow"
            }
        ]
    }
}
